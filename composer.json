{
  "require": {
    "php": ">=8.3",
    "monolog/monolog": "^3.7",
    "twig/twig": "^3.10",
    "php-kit/ext-pdo": "^1.1",
    "ext-pdo": "*",
    "doctrine/orm": "^3.2",
    "doctrine/migrations": "^3.8",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/dbal": "^4.1",
    "php-http/guzzle7-adapter": "^1.0",
    "nyholm/psr7": "^1.8",
    "mailersend/mailersend": "^0.27.0",
    "fakerphp/faker": "^1.23"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "doctrine/data-fixtures": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "psr-0": {
      "App\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
